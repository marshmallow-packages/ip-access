{
    "name": "marshmallow/ip-access",
    "description": "Marshmallow - Laravel IP Access",
    "keywords": [
        "marshmallow",
        "ip-access",
        "laravel nova"
    ],
    "homepage": "https://github.com/marshmallow/ip-access",
    "license": "MIT",
    "authors": [
        {
            "name": "LTKort",
            "email": "lars@marshmallow.dev",
            "role": "Developer"
        },
        {
            "name": "Stef van Esch",
            "email": "stef@marshmallow.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.0|^9.0|^10.0|^11.0",
        "marshmallow/commands": "^1.2",
        "marshmallow/helpers": "^2.0",
        "laravel/nova": "^4.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require-dev": {
        "orchestra/testbench": "^9.1.2",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Marshmallow\\IpAccess\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Marshmallow\\IpAccess\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Marshmallow\\IpAccess\\IpAccessServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
